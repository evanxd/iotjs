{
  "source": {
    "include": ["README.md", "lib/"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "templates": {
    "applicationName": "Thing.js API Doc",
    "googleAnalytics": "UA-81619415-1",
    "meta": {
      "title": "Thing.js API Doc",
      "description": "Thing.js API doc."
    }
  },
  "opts": {
    "template": "node_modules/jaguarjs-jsdoc",
    "encoding": "utf8",
    "destination": "./doc/"
  }
}
