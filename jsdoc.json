{
  "source": {
    "include": ["README.md", "module/"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "templates": {
    "applicationName": "IoT.js Module Doc",
    "googleAnalytics": "UA-82888912-1",
    "meta": {
      "title": "IoT.js Module Doc",
      "description": "IoT.js module doc."
    }
  },
  "opts": {
    "template": "node_modules/jaguarjs-jsdoc",
    "encoding": "utf8",
    "destination": "./doc/"
  }
}
